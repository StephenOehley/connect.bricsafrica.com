@{
    ViewBag.Title = "ChangePassword";
    Layout = "~/Views/Shared/_BusinessGeneratorLayout.cshtml";
}

<div class="form-profile text-allcontent">
<form action="/Account/ChangePassword" id="signupform" method="post" class="form-padding">
<h1 class="text-be-formcontent">Change Password</h1>
<br />
<label for="passwordold">Old Password</label>
<br />
<input accesskey="n" class="required" id="passwordold" name="passwordold" style="width: 200px" type="password" value="" autocomplete="off" /><br />
<br />
<label for="passwordnew">New Password</label>
<br />
<input accesskey="n" class="required" id="passwordnew" name="passwordnew" style="width: 200px" type="password" value="" autocomplete="off" /><br />
<br />
<br />
<input type="submit" id="bSignup" name="bSignup" value="Change" accesskey="s" class="form-button" />
<br />
<br />
<p>@ViewBag.Message</p>
<br />
<br />
</form>
</div>

